{{- $.Scratch.Add "index" slice -}}
{{- range where .Site.RegularPages "Params.layout" "!=" "blank"  -}}
{{- $.Scratch.Add "index" (dict "title" .Title "tags" .Params.tags "categories" .Params.categories "contents" .Plain "permalink" .Permalink "html" .Content) -}}
{{- end -}}
{{- $.Scratch.Get "index" | jsonify -}}
